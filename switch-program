#include<ctype.h>
void main( )
{
	char grade;

	printf("Enter the grade tof coffee: ");
	scanf("%c",&grade);
//fn toupper converts LETTERS TOupper case to lower case but temporary
grade = getch(grade);
	switch(grade)
	{
	case 'A': printf("\nExport quality");
		  break;
	case 'B': printf("\nlocal market");
		  break;
	case 'C':
	case 'D': printf("\nGood for Blending");
		  break;
	default: printf("\n%c Rejected",grade);  
	      break; 
	}
	printf("\n\n");
}
